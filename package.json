{
  "name": "unassert-cli",
  "description": "CLI for unassert: provides `unassert` command which compiles assertions away from target file",
  "version": "1.0.0",
  "author": {
    "name": "Takuto Wada",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "bin": {
    "unassert": "./bin/cmd.js"
  },
  "bugs": "https://github.com/unassert-js/unassert-cli/issues",
  "dependencies": {
    "acorn": "^8.0.0",
    "concat-stream": "^2.0.0",
    "escodegen": "^2.0.0",
    "unassert": "^1.3.1"
  },
  "devDependencies": {
    "mocha": "^10.0.0",
    "semistandard": "^16.0.1",
    "snazzy": "^9.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "bin",
    "package.json"
  ],
  "homepage": "https://github.com/unassert-js/unassert-cli",
  "keywords": [
    "DbC",
    "assert",
    "assertion",
    "unassert"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unassert-js/unassert-cli.git"
  },
  "scripts": {
    "lint": "semistandard --verbose bin/*.js test/*.js | snazzy",
    "fmt": "semistandard --fix bin/*.js test/*.js",
    "test": "npm run lint && mocha test"
  },
  "semistandard": {
    "globals": [
      "describe",
      "beforeEach",
      "it"
    ]
  }
}
